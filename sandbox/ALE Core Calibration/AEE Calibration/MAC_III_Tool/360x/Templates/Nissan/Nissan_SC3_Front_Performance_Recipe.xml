<?xml version="1.0" encoding="utf-8" ?> 
<!--  TEMPLATE FOR MAC 3.6.x.x and later -->
<!--  Works with "Nissan_SC3_Report_Template.xls" PTC version 1.1 or later -->
<!--  Nissan_SC3_Front_Performance_Recipe.xml (PTC v1.1) -->


<MAC_REPORT_RECIPE	template="Nissan_SC3_Report_Template.xlsx"
					language="English">

	<USER_DEFINED_SYMBOL	name="performance_title" 		value="Front Calibration Performance Summary" />
    <USER_DEFINED_SYMBOL	name="activation_handler_name" 	value="TBD" />
	<USER_DEFINED_SYMBOL	name="calibrator_name" 			value="TBD" />
	<USER_DEFINED_SYMBOL	name="study_type" 				value="Insert Model Year and Vehicle ID" />
    <USER_DEFINED_SYMBOL	name="event_number" 			value=" " />
	<USER_DEFINED_SYMBOL	name="module_architecture" 		value="SC3_Nissan" />

	<SHEET		source="Veoneer_sh1"
				target="Front Performance"				
				iterator="none"
				repeat="forever"
				iterator_state="init">
			
		<PAGE		range="pa1" 
					iterator="none" 
					repeat="forever"
					fixed_areas_max_index="3"
					iterator_state="init">

			<STATIC_CELL	name="performance_title" />
			<STATIC_CELL	name="calibration_file_name" />
			<STATIC_CELL	name="study_type" />

			<STATIC_CELL 	name="calibration_tool_and_version" />
			<STATIC_CELL 	name="module_architecture" />
<!--
			<STATIC_CELL 	name="general_information_unit_type" />
-->
<!--
			<STATIC_CELL 	name="activation_handler_name" />
-->
			<STATIC_CELL 	name="general_information_computation_date" />
			<STATIC_CELL 	name="calibrator_name" />
<!--

			<STATIC_CELL 	name="general_information_data_validity_message" />			
-->			
			<ARRAY		range="ar0" 
						iterator="none"
						iterator_state="init">
				<VERTICAL_DIVISION	range="vd1"
									iterator="none"
									iterator_state="init"
									repeat="forever"
									title_cell_name="fixed">
					<HORIZONTAL_DIVISION	range="hd1"
											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">
						<BLOCK	range="bl"
								type="horizontal"
								iterator="drift"
								iterator_state="init"
								repeat="forever">
							
							<DYNAMIC_CELL name= "drift_summary_information"/>
							
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
			</ARRAY>

			<ARRAY		range="ar2" 
						iterator="none"
						iterator_state="init">
				<VERTICAL_DIVISION	range="vd1"
									iterator="none"
									iterator_state="init"
									repeat="once"
									title_cell_name="">
							
					<HORIZONTAL_DIVISION	range="hd1"
											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">
						<BLOCK	range="bl"
								type="horizontal"
								iterator="file"
								iterator_state="init"
								repeat="forever">
								
 								<DYNAMIC_CELL name="event_number"/>
 								<DYNAMIC_CELL name="file_vehicle_code"/>
							    <DYNAMIC_CELL name="file_test_reference"/>
								<DYNAMIC_CELL name="file_name"/>
								
								<STATIC_TITLE_CELL name="fixed" index="1"/>
								<STATIC_TITLE_CELL name="fixed" index="2"/>
								<STATIC_TITLE_CELL name="fixed" index="5"/>
								<STATIC_TITLE_CELL name="fixed" index="7"/>
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
				

				<VERTICAL_DIVISION	range="vd2"
									iterator="time_event"
									iterator_state="init"
									repeat="forever"
									title_cell_name="time_event_name"	
									time_event_category_filter = "TTF"	
									time_event_algorithm_filter = "Front">
							
					<HORIZONTAL_DIVISION	range="hd1"
											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">

						<BLOCK	range="bl"
								type="horizontal"
								iterator="file"
								iterator_state="init"
								repeat="forever">
							
						    
							<DYNAMIC_CELL name="performance_study_min" out_of_range_performance_style="cell_background_yellow" no_fire_performance_and_optimal_style="cell_background_black"/>
							<DYNAMIC_CELL name="performance_study_nom" out_of_range_performance_style="cell_background_yellow" no_fire_performance_and_optimal_style="cell_background_black"/>
							<DYNAMIC_CELL name="performance_study_max" out_of_range_performance_style="cell_background_yellow" no_fire_performance_and_optimal_style="cell_background_black"/>
							<DYNAMIC_CELL name="performance_study_nf_presence"/>
						    <DYNAMIC_CELL name="performance_study_opt_min"/>
<!--							<DYNAMIC_CELL name="performance_study_opt_nom"/>
-->
							<DYNAMIC_CELL name="performance_study_opt_max"/>
							
							<STATIC_TITLE_CELL name="fixed" index="1"/>
							<STATIC_TITLE_CELL name="fixed" index="2"/>
							<STATIC_TITLE_CELL name="fixed" index="3"/>
							<STATIC_TITLE_CELL name="fixed" index="4"/>
							<STATIC_TITLE_CELL name="fixed" index="5"/>
<!--							<STATIC_TITLE_CELL name="fixed" index="6"/>
-->
							<STATIC_TITLE_CELL name="fixed" index="7"/>
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
			</ARRAY>
		</PAGE>
	</SHEET>


	<SHEET		source="Veoneer_sh1_sev"
