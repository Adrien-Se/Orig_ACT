											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">
						<BLOCK	range="bl"
								type="horizontal"
								iterator="drift"
								iterator_state="init"
								repeat="forever">
							
							<DYNAMIC_CELL name= "drift_summary_information"/>
							
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
			</ARRAY>

			<ARRAY		range="ar2" 
						iterator="none"
						iterator_state="init">
				<VERTICAL_DIVISION	range="vd1"
									iterator="none"
									iterator_state="init"
									repeat="once"
									title_cell_name="">
							
					<HORIZONTAL_DIVISION	range="hd1"
											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">
						<BLOCK	range="bl"
								type="horizontal"
								iterator="file"
								iterator_state="init"
								repeat="forever">
								
								<DYNAMIC_CELL name="file_vehicle_code"/>
								<DYNAMIC_CELL name="file_test_reference"/>
								<DYNAMIC_CELL name="file_name"/>
								
								<STATIC_TITLE_CELL name="fixed" index="1"/>
								<STATIC_TITLE_CELL name="fixed" index="5"/>
								<STATIC_TITLE_CELL name="fixed" index="7"/>
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
				

				<VERTICAL_DIVISION	range="vd2"
									iterator="time_event"
									iterator_state="init"
									repeat="forever"
									title_cell_name="time_event_description"	
									time_event_category_filter = "Severity"	
									time_event_algorithm_filter = "Front">
							
					<HORIZONTAL_DIVISION	range="hd1"
											iterator="none"
											iterator_state="init"
											repeat="forever"
											title_cell_name="">

						<BLOCK	range="bl"
								type="horizontal"
								iterator="file"
								iterator_state="init"
								repeat="forever">
							
 							<DYNAMIC_CELL name="performance_study_min"/>
							<DYNAMIC_CELL name="performance_study_nom"/>
							<DYNAMIC_CELL name="performance_study_max"/>
						    <DYNAMIC_CELL name="performance_study_nf_presence"/>
<!--						<DYNAMIC_CELL name="performance_study_opt_nom"/>
							<DYNAMIC_CELL name="performance_study_opt_max"/>
-->
							
							<STATIC_TITLE_CELL name="fixed" index="1"/>
							<STATIC_TITLE_CELL name="fixed" index="2"/>
							<STATIC_TITLE_CELL name="fixed" index="3"/>
						    <STATIC_TITLE_CELL name="fixed" index="4"/>
<!--						<STATIC_TITLE_CELL name="fixed" index="5"/>
							<STATIC_TITLE_CELL name="fixed" index="6"/>
-->
  
						</BLOCK>
					</HORIZONTAL_DIVISION>
				</VERTICAL_DIVISION>
			</ARRAY>

		</PAGE>
	</SHEET>


